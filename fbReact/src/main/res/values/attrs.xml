<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ReactBadge">
        <attr name="srcReact" format="enum">
            <enum name="like" value="0" />
            <enum name="love" value="1" />
            <enum name="haha" value="2" />
            <enum name="wow" value="3" />
            <enum name="sad" value="4" />
            <enum name="angry" value="5" />
        </attr>
        <attr name="reactCount" format="integer" />
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="animationEnabled" format="boolean" />
        <attr name="animationDuration" format="integer" />
        <attr name="enableNumberConvert" format="boolean" />
        <attr name="background" format="reference" />
    </declare-styleable>
</resources>